<!DOCTYPE html>
 <html>
   <head>
     <title><%= title %></title>
     <%= csrf_meta_tag %>
     <%= render 'layouts/stylesheets' %>
     <%= javascript_include_tag "application" %>
     <%= javascript_include_tag "customglobal" %>
  </head>
  <body>
     <div class="container">
       <%= render 'layouts/header' %>
        <section class="round">
        <% flash.each do |key,val| %>
          <div class="flash <%= key %>"><%= val %></div>
        <% end %>
          <%= yield %>
       </section>
       <%= render 'layouts/footer' %>
       <%= debug(params) if Rails.env.development? %>
  </body>
</html>
 
