<!DOCTYPE html>
 <html>
   <head>
     <title><%= title %></title>
     <%= csrf_meta_tag %>
     <%= render 'layouts/stylesheets' %>
     <%= javascript_include_tag "application" %>
     <%= yield(:head) %>
  </head>
  <body>
     <div class="container">
       <%= render 'layouts/header' %>
        <section class="round">
        <%= render :partial => 'shared/flash' %>
          <%= yield %>
       </section>
       <%= render 'layouts/footer' %>
       <%= debug(params) if Rails.env.development? %>
  </body>
</html>
 
