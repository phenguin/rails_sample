<header>
<div id="navsection">
  <div class="innertube round">
    <nav>
    <div class="groupnav">
      <ul>
        <li><%= link_to "Create a group!", new_group_path, :class => "round filled" %></li>
        <li><%= link_to "Find a group!", groups_path, :class => "round filled" %></li>
      </ul>
    </div>
    <div class="usernav">
      <ul>
        <li><%= link_to "Home", root_path %></li>
        <% if signed_in? %>
          <li><%= link_to "Settings", edit_user_path(@current_user) %></li>
          <li><%= link_to "Users", users_path %></li>
          <li><%= link_to "Profile", user_path(@current_user) %></li>
          <li><%= link_to "Sign out", signout_path, :method => :delete %></li>
        <% else %>
          <li><%= link_to "Log in", signin_path %></li>
          <li><%= link_to "Sign up!", signup_path %></li>
        <% end %>
      </ul>
    </div>
    </nav>
  </div>
</div>
<div id="topsection">
  <div class="innertube">
    <% logo = image_tag("logo.png", :alt => "Group-Organize", :class => "round") %>
  </div>
</div>
</header>
